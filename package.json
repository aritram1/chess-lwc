{
  "name": "chessgame2",
  "version": "1.0.0",
  "description": "A simple Webpack project",
  "scripts": {
    "build-dev": "webpack --mode=development",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve-dev": "npm run build-dev && webpack serve --open --mode=development"
  },
  "nodemon": {
    "watch": [
      "src"
    ],
    "ext": "js,json,css,html",
    "ignore": [
      "node_modules"
    ],
    "execMap": {
      "js": "node --inspect"
    },
    "env": {
      "NODE_ENV": "development"
    },
    "delay": "1000",
    "verbose": true
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  },
  "dependencies": {
    "@chrisoakman/chessboardjs": "^1.0.0",
    "chess.js": "^1.0.0-beta.4",
    "jquery": "^3.6.4"
  }
}
